{
  "name": "@permaweb/ao-cu",
  "version": "1.0.0",
  "description": "A Node Express server that implements the ao Compute Unit specification",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon",
    "docker-build": "docker build -t cu-app .",
    "docker-run": "npm run docker-build && docker run -p 3005:3005 cu-app",
    "start": "node src/app.js",
    "test": "node --test ./src"
  },
  "dependencies": {
    "@permaweb/ao-loader": "0.0.7",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "hyper-async": "^1.1.2",
    "ms": "^2.1.3",
    "pouchdb": "^8.0.1",
    "pouchdb-adapter-leveldb": "^8.0.1",
    "pouchdb-find": "^8.0.1",
    "ramda": "^0.29.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": "20"
  }
}
