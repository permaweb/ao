{
  "name": "@permaweb/ao-sdk",
  "version": "0.0.3",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {
    "./dist/index.cjs": "./dist/browser.cjs",
    "./dist/index.js": "./dist/browser.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:src",
    "build:src": "node esbuild.js",
    "build:types": "tsc src/index.js --skipLibCheck --declaration --allowJs --emitDeclarationOnly --outDir dist",
    "test": "node --test ./src",
    "pretest:integration": "npm run build && npm link",
    "test:integration": "cd ./test/e2e && npm test"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "debug": "^4.3.4",
    "hyper-async": "^1.1.2",
    "ramda": "^0.29.0",
    "warp-arbundles": "^1.0.4",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "arweave": "^1.14.4",
    "esbuild": "^0.19.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
